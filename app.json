{
  "expo": {
    "name": "compliance-diary",
    "slug": "compliance-diary",
    "version": "1.0.3",
    "orientation": "portrait",
    "scheme": "compliance-diary",
    "userInterfaceStyle": "automatic",

    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thirdeyetechlabs.compliancediary"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": ["com.android.vending.BILLING"],
      "package": "com.thirdeyetechlabs.compliancediary"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "react-native-iap",
      "./components/with-iap"
    ],
    "experiments": {
      "typedRoutes": true
    },

    "deepLinking": {
      "prefixes": [
        "compliance-diary://",
        "https://play.google.com/store/apps/details?id=com.thirdeyetechlabs.compliancediary",
        "https://apps.apple.com/app/id1234567890"
      ],
      "config": {
        "screens": {
          "Home": "home/:param"
        }
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "426e07e6-3cc0-4325-b6d6-af46f91438a1"
      }
    },
    "jsEngine": "hermes"
  }
}
